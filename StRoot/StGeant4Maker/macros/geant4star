#!/bin/csh -f 

setenv __G4STAR_MYSQL_DIR__ `mysql_config --variable=pkglibdir`

setenv ROOT_INCLUDE_PATH ${STAR}:${STAR}/StRoot:${STAR}/StRoot/StStarLogger:${STAR}/StRoot/StGeant4Maker:${STAR}:StRoot/StarGenerator/BASE:${STAR}/StarVMC/StarAgmlLib/:${STAR}/.sl73_x8664_gcc485/include/

if ( -e ./StRoot/StGeant4Maker/macros/geant4star.C ) then
   root.exe -l ./StRoot/StGeant4Maker/macros/initStar.C ./StRoot/StGeant4Maker/macros/geant4star.C $argv 
else
   root.exe -l $STAR/StRoot/StGeant4Maker/macros/initStar.C $STAR/StRoot/StGeant4Maker/macros/geant4star.C $argv 
endif



