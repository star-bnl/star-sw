<?xml version='1.0' encoding='UTF-8' ?>

<!-- This file contains information over-riding dataDefault for
     year 2001 minbias data analysis. -->
<jobDescription>
    <standardElements>
        <jobControl>
            <jobName widget='entry' id='jobName'>CuCu200_2007ib_cuProductionMinBias</jobName>
            <outputDir widget='entry' id='outputDir'>$env(MYDATA)/Data/cucu200/2007ib/cuProductionMinBias</outputDir>
            <jobPurpose widget='text'>Run over year 5, 200GeV CuCu minimum bias data produced with 2007ib library.
            </jobPurpose>
        </jobControl>

        <starSubmit simulateSubmission='true'>
            <input URL="catalog:star.bnl.gov?production=P07ib,trgsetupname=cuProductionMinBias,filetype=daq_reco_MuDst,tpc=1,sanity=1,filename~st_physics" singleCopy="true" nFiles="all" />
        </starSubmit>

        <eventCuts>
            <triggerTag Comment = 'Trigger tag for 2007ib minbias CuCu data at 200GeV'>CuCu200GeVProductionMinBias2007ib</triggerTag>
            <eventCut Comment = 'cut on position of primary vertex'>
                <cutName>primaryVertexZ</cutName>
                <Value widget = 'entry'>-30</Value>
                <Value widget = 'entry'>30</Value>
            </eventCut>
        </eventCuts>

        <doEStructMacro>
            <centralities id='centralities' widget='entry'>0 25 53 91 143 215 265 400</centralities>
            <ptCentralities id='ptCentralities' typedef='const  double ptMultCut[] = ' typedef2='int mbNPtBins = '>0 25 53 91 143 215 265 400</ptCentralities>
        </doEStructMacro>
    </standardElements>
</jobDescription>
