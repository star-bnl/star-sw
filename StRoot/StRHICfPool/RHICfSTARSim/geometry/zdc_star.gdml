    <box name="Log-zdc-base" lunit="cm" x="zdc0*2.0" y="zdc1*2.0" z="zdc2*2.0"/>
    <box name="Log-zdc-sub" lunit="cm" x="gap0*2.0+1" y="(gap1+gap2)*2.0" z="(gap1+gap2)*2.0"/>
