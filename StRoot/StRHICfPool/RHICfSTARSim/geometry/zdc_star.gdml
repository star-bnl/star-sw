    <box name="Log-smdh-strip-main" lunit="mm" x="110" y="10" z="7"/>
    <box name="Log-smdh-strip-sub" lunit="mm" x="111" y="70/sqrt(74)" z="12"/>

    <subtraction name="Log-smdh-strip-00">
