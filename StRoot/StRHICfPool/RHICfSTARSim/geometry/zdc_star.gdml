      <first ref="Log-zdc-base"/>
      <second ref="Log-zdc-sub"/>
      <positionref ref="Pos-zdc-sub-0"/>
      <rotationref ref="Rot-45"/>
    </subtraction>
    <subtraction name="Log-zdc">
