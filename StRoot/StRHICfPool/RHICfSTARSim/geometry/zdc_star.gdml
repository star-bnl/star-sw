    <subtraction name="Log-zdc2">
      <first ref="Log-zdc2-0"/>
      <second ref="Log-zdc-sub"/>
      <positionref ref="Pos-zdc-sub-1"/>
      <rotationref ref="Rot-45"/>
    </subtraction>

    <!-- Iron plate -->
    <box name="Log-ipl" lunit="cm" x="ipl0*2.0" y="ipl1*2.0" z="ipl2*2.0"/>
