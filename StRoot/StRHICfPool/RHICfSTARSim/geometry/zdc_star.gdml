      <second ref="Log-zdc-sub"/>
      <positionref ref="Pos-zdc-sub-1"/>
      <rotationref ref="Rot-45"/>
    </subtraction>

    <box name="Log-zdc-base2" lunit="cm" x="zdc0*2.0-1" y="zdc1*2.0" z="zdc2*2.0"/>
