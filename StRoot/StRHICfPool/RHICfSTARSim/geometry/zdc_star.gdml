    <variable name="gap0" value="5.0"/>
    <variable name="gap1" value="0.09"/>
    <variable name="gap2" value="20.0"/>

    <variable name="nlay" value="26"/>
    <variable name="lmod" value="(2.0*ipl1+(nlay+1)*wpl1+nlay*gap1)/sin45"/>

    <variable name="zdc0" value="5.0"/>
    <variable name="zdc1" value="gap2*cos45+gap1*sin45"/>
    <variable name="zdc2" value="gap2*sin45+gap1*cos45+lmod"/>
 
