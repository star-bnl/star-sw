    <box name="Log-wpl" lunit="cm" x="wpl0*2.0" y="wpl1*2.0" z="wpl2*2.0"/>
    <!-- GAPF: gap which has fibers -->
    <box name="Log-gapf" lunit="cm" x="gap0*2.0" y="gap1*2.0" z="gap2*2.0"/>
    <!-- Half of PMMA fiber -->
    <tube name="Log-fibr" rmin="fib0" rmax="fib1" z="fib2*2" deltaphi="360" startphi="0" aunit="deg" lunit="cm"/>

    <!-- SMD -->
    <box name="Log-smd-gap-base" lunit="cm" x="smdg0*2.0" y="smdg1*2.0" z="smdg2*2.0"/>
    <box name="Log-smd-sub" lunit="cm" x="smdg0*2.0+1" y="(gap1+gap2)*2.0" z="(gap1+gap2)*2.0"/>
