<?xml version="1.0" encoding="utf-8" ?>
<job minFilesPerProcess="10" maxFilesPerProcess="100" filesPerHour="50" fileListSyntax="xrootd" simulateSubmission ="false" copyInputLocally="false">
        <command>
  set CURRENT_DIR = `pwd`
#  cd ${SUBMITTINGDIRECTORY}
  setenv STARFPE NO
foreach item (`cat $FILELIST`)
  set input  = `echo ${item} | awk '{print $1}'`
  xrdcopy ${input} .
end
#  set input  = `echo ${item} | awk '{print $1}'`
  set file   = `echo ${input} | sed -e '#/root://xrdstar.rcf.bnl.gov:1095/##'`
  set filel  = `basename ${file} .picoDst.root`;
  set log    = ${filel}B.log
  set output = ${filel}.root
  set logF   = ${SUBMITTINGDIRECTORY}/${filel}B.log
  set outputF =  ${SUBMITTINGDIRECTORY}/${filel}.root
  set bla     =  ${SUBMITTINGDIRECTORY}/${filel}.bla
  if (-r $bla) continue;
  if (-r ${logF}) continue;
  if (-r ${outputF}) continue;
  touch ${bla}
  set idNdx = kFALSE;
#  root.exe -q -b -x  'kfpAnalysis.C(1000000,"'${input}'","'${output}'","y2018",'${idNdx}')' &gt;&amp; ${log}
  root.exe -q -b -x  'kfpAnalysis.C(1000000,"*.picoDst.root","'${output}'","y2018",'${idNdx}')' &gt;&amp; ${log}
  rm ${bla}
  mv ${log} ${output}  ${SUBMITTINGDIRECTORY}/
        </command>
    <ResourceUsage>
      <Priority>99</Priority>
    </ResourceUsage>
    <stdout URL="file:./sched$JOBID.log" />
    <stderr URL="file:./sched$JOBID.err" />
