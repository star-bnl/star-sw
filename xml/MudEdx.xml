<job maxFilesPerProcess="20" filesPerHour="100" fileListSyntax="paths">
	<command>
         cd ${SUBMITTINGDIRECTORY}
         setenv NODEBUG yes
         setenv STARFPE NO
         setup 64b
         starver .DEV2
         root.exe -q -b lMuDst.C 'MudEdx.C+("'${FILELIST}'","'${JOBID}'.root")'  &gt;&amp; ${JOBID}B.log 
	</command>
	<stdout URL="file:./sched$JOBID.log" />
	<stderr URL="file:./sched$JOBID.err" />
