macro tfc
macro/global/import *

  cd [raw_data]/tpc/pixels
  
  tdm/newtable adcxyz  tfc_adcxyz   900000
    
  cd [params]/tpc
  mkdir tfcpars
  cd tfcpars
  tdm/newtable tfc_sector_index tcl_sector_index 1 
  
  cd /dui

return

