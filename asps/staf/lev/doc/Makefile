# SLMakefile.doc - put in document directory of package (pkg/doc)
#?--------- pkg/doc/Makefile - generate package documentation ---------
# DO NOT EDIT THIS FILE - USE "make reset" FROM BASE DIRECTORY OR
# EDIT THE USER FILES Makefile.control AND Makefile.user
#######################################################################
ifeq ($(EMPTY),$(strip $(BASE_DIR)))
include ../*.config
endif
#
.PHONY: all clean test help install default
#
VERSION                 := "$Id: Makefile,v 1.5 1997/12/12 20:00:31 tull Exp $"
#
default: all
#
all:
	@echo "########################################################"
	@echo "###                No documents made.                ###"
	@echo "########################################################"
#
clean:
test:
install:
#
help:
	@$(SLM_DIR)/slm_help $(SLM_DIR)
#
Makefile: $(SLM_DIR)/SLMakefile.doc
	cp $< $@
#
