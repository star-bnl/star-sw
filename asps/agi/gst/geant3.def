          [; %TITLE = '#3'; %EXNAME = '#2'; %ParList = '#1';Call Ax#2;"*"; ]
  REPLACE [;<#>[GSYS0][10SYS] [KSYS0]=#;]      with  [; %#4 = #5; <#1_#4>#2 #3; ]
