      <Do var="jj" from="1" to="SCSD_numberOfCables">
        <Create    block="SCAD" />  
        <Placement block="SCAD" x="SCSD_xpos(jj)" y="SCSD_ypos(jj)" z="zoffCableS" />  
       </Do>

     <Do var="ll" from="1" to="LCSD_numberOfCables">
