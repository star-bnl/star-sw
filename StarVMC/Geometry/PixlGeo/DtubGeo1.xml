      <Create    block="LSAJ" />  
      <Placement block="LSAJ" x="-8.1002" y="0.3995" z="-38.9488" />
      <Create    block="LSAK" />  
      <Placement block="LSAK" x="-8.1001" y="-0.4002" z="-39.287" />
      <Create    block="LSAL" />  
      <Placement block="LSAL" x="-8.0213" y="-1.196" z="-39.6531" />
      <Create    block="LSAM" />  
      <Placement block="LSAM" x="-7.8645" y="-1.9802" z="-40.0446" />
      <Create    block="LSAN" />  
      <Placement block="LSAN" x="-7.6313" y="-2.7451" z="-40.4339" />
      <Create    block="LSAO" />  
      <Placement block="LSAO" x="-7.3239" y="-3.4833" z="-40.7774" />
      <Create    block="LSAP" />  
      <Placement block="LSAP" x="-6.9452" y="-4.1876" z="-41.1379" />
      <Create    block="LSAQ" />  
      <Placement block="LSAQ" x="-6.499" y="-4.8513" z="-41.5212" />
      <Create    block="LSAR" />  
