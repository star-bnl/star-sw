   <Rotation alphay="90"/>
   <Rotation alphax="-90"/>
   <Rotation alphaz="180"/>
   </Placement>

   <Create    block="ATBI"/>
   <Placement block="ATBI" x="-0.8-Thk/2." y="-5.0705" z="15.0806+zoffd">
   <Rotation alphay="90"/>
   <Rotation alphax="-90"/>
   </Placement>

   <Create    block="DCTH" />
   <Placement block="DCTH" x="-0.8-Thk/2.0+Thk/2.-0.32+0.02" y="2.4967+Thk/2.+0.32-0.02" z="DSEA/2.+DSEB+(DSEC+DSED)/2.+zoffd"/> 

   <Create    block="TCTH" />
   <Placement block="TCTH" x="-0.8-Thk/2.0+Thk/2.-0.32+0.02" y="-1*(2.4967+Thk/2.+0.32-0.02)" z="DSEA/2.+DSEB+(DSEC+DSED)/2.+zoffd"/> 
 
  <!-- this piece is adjusted by hand -->
   <Create    block="CSTI" />
   <Placement block="CSTI" x="-.8-.2*Thk-.32+.02" y="-1*(2.4967+2*Thk+.4)" z="DSEA/2.+DSEB/2.+.1+zoffd">
   <Rotation alphax="-8.75"/>
   <Rotation alphay="6"/>
   </Placement> 

   <Create    block="CSBI" />
   <Placement block="CSBI" x="-.8-.2*Thk-.32+.02" y="(2.4967+2*Thk+.4)" z="DSEA/2.+DSEB/2.+.1+zoffd">
   <Rotation alphax="8.75"/>
   <Rotation alphay="6"/>
   </Placement> 

   <Create    block="APET" />
   <Placement block="APET" x="-0.8-Thk/2." y="4.9624" z="DSEA/2.+DSEB+DSEC+DSED/2.+zoffd"/>

   <Create    block="APEB" />
   <Placement block="APEB" x="-0.8-Thk/2." y="-4.9624" z="DSEA/2.+DSEB+DSEC+DSED/2.+zoffd"/>

   <Create    block="OCYE" />
   <Placement block="OCYE" x="0" y="0" z="DSEA/2.+DSEB+DSEC+(DSED-0.475)/2.+zoffd"/>

   <Create    block="OCEA" />
   <Placement block="OCEA" x="0" y="0" z="DSEA/2.+DSEB+DSEC+DSED-0.475/2.+zoffd"/>

   <Create    block="OCEB" />
   <Placement block="OCEB" x="0" y="0" z="DSEA/2.+DSEB+DSEC+DSED-0.475/2.+zoffd"/>

   <Create    block="OCEC" />
   <Placement block="OCEC" x="0" y="0" z="DSEA/2.+DSEB+DSEC+DSED-0.475/2.+zoffd"/>

   <Create    block="OCED" />
   <Placement block="OCED" x="0" y="0" z="DSEA/2.+DSEB+DSEC+DSED-0.475/2.+zoffd"/>

   <Create    block="UPTE" />
   <Placement block="UPTE" x="-1.22" y="7.0756" z="DSEA/2.+DSEB+DSEC+DSED/2.+zoffd"/>

   <Create    block="BOTE" />
   <Placement block="BOTE" x="-1.22" y="-7.0756" z="DSEA/2.+DSEB+DSEC+DSED/2.+zoffd"/>

   <!--cables -->

   <!-- placement of cables section A -->

      <Create    block="LSAA" />  
      <Placement block="LSAA" x="-4.8024" y="6.5352" z="-35.9334" />
      <Create    block="LSAB" />  
      <Placement block="LSAB" x="-5.4216" y="6.0305" z="-36.2522" />
      <Create    block="LSAC" />  
      <Placement block="LSAC" x="-5.9902" y="5.4672" z="-36.5991" />
